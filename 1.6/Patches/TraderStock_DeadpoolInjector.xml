<?xml version="1.0" encoding="utf-8"?>

<Patch>
  <!-- RimWorld 1.6 vanilla trader kinds -->
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="OrbitalExotic"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_SingleDef">
        <thingDef>DP_HealingInjector</thingDef>
        <countRange>
          <min>1</min>
          <max>1</max>
        </countRange>
      </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>Defs/TraderKindDef[defName="CivilOutlander_HighTech"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_SingleDef">
        <thingDef>DP_HealingInjector</thingDef>
        <countRange>
          <min>1</min>
          <max>1</max>
        </countRange>
      </li>
    </value>
  </Operation>
</Patch>



