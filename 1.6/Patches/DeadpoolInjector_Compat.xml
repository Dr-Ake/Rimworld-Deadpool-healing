<?xml version="1.0" encoding="utf-8"?>

<Patch>
  <!-- Replace stray references to DP_HealingInjector1 with DP_HealingInjector in any loaded XML. -->
  <Operation Class="PatchOperationReplace">
    <xpath>//thingDef[text()='DP_HealingInjector1']</xpath>
    <value>
      <thingDef>DP_HealingInjector</thingDef>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>//li[text()='DP_HealingInjector1']</xpath>
    <value>
      <li>DP_HealingInjector</li>
    </value>
  </Operation>
</Patch>


